{
    "name": "slack-clone",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "lint": "next lint",
        "db:generate": "dotenv -e .env.local npx prisma generate dev",
        "db:migrate": "dotenv -e .env.local npx prisma migrate dev",
        "db:push": "dotenv -e .env.local npx prisma db push",
        "db:seed": "dotenv -e .env.local npx prisma db seed",
        "db:reset": "dotenv -e .env.local npx prisma migrate reset",
        "db:studio": "dotenv -e .env.local npx prisma studio"
    },
    "dependencies": {
        "@clerk/nextjs": "^4.21.14",
        "@clerk/themes": "^1.7.5",
        "@paralleldrive/cuid2": "^2.2.1",
        "@prisma/client": "^5.0.0",
        "@radix-ui/react-collapsible": "^1.0.3",
        "@radix-ui/react-dropdown-menu": "^2.0.5",
        "@radix-ui/react-slot": "^1.0.2",
        "@types/react": "18.2.14",
        "@types/react-dom": "18.2.6",
        "class-variance-authority": "^0.7.0",
        "clsx": "^2.0.0",
        "lucide-react": "^0.261.0",
        "next": "13.4.9",
        "next-themes": "^0.2.1",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "socket.io": "^4.7.1",
        "socket.io-client": "^4.7.1",
        "tailwind-merge": "^1.14.0",
        "tailwindcss-animate": "^1.0.6"
    },
    "devDependencies": {
        "@types/node": "20.4.0",
        "autoprefixer": "10.4.14",
        "postcss": "8.4.25",
        "prettier": "^3.0.0",
        "prettier-plugin-tailwindcss": "^0.3.0",
        "prisma": "^5.0.0",
        "supports-color": "8.1.1",
        "tailwindcss": "3.3.2",
        "ts-node": "^10.9.1",
        "typescript": "5.1.6"
    },
    "prisma": {
        "seed": "ts-node --transpile-only prisma/seed.ts"
    }
}
