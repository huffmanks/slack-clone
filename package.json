{
    "name": "nextjs-with-prisma",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "lint": "next lint",
        "generate": "dotenv -e .env.local npx prisma generate",
        "migrate": "dotenv -e .env.local npx prisma migrate dev",
        "push": "dotenv -e .env.local npx prisma db push",
        "seed": "dotenv -e .env.local npx prisma db seed",
        "reset": "dotenv -e .env.local npx prisma migrate reset",
        "studio": "dotenv -e .env.local npx prisma studio"
    },
    "dependencies": {
        "@auth/prisma-adapter": "^1.0.0",
        "@prisma/client": "^4.16.1",
        "@types/node": "20.2.5",
        "@types/react": "18.2.7",
        "@types/react-dom": "18.2.4",
        "autoprefixer": "10.4.14",
        "dotenv-cli": "^7.2.1",
        "global": "^4.4.0",
        "next": "^13.4.7",
        "next-auth": "^4.22.1",
        "postcss": "8.4.24",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "tailwindcss": "3.3.2",
        "typescript": "5.0.4"
    },
    "devDependencies": {
        "prettier": "^2.8.8",
        "prettier-plugin-tailwindcss": "^0.3.0",
        "prisma": "^4.16.1",
        "ts-node": "^10.9.1"
    },
    "prisma": {
        "seed": "ts-node --transpile-only prisma/seed.ts"
    }
}
